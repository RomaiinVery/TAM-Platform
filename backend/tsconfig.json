{
    "compilerOptions": {
      "module": "esnext",
      "target": "es2021",
      "moduleResolution": "node",
      "outDir": "dist",
      "rootDir": "src",
      "strict": true,
      "esModuleInterop": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "skipLibCheck": true,
      "types": ["node"]
    },
    "include": ["src"]
  }
  